{
    "manifest_version":2,
    "name":"PlugAnotation",
    "version":"1.0",
    "description":"Plugin para anotar",
    "icons":{
        "128":"LOGO_1_ANAGRAMA.png"

    },
    "background":{
        "scripts":["jquery341min.js","context.js"],
        "persistent":false
    },
    "browser_action":{
        "default_icon":"LOGO_1_ANAGRAMA.png",
        "default_title":"PlugAnotation",
        "default_popup":"popup.html"
    },
    "permissions":[
        "tabs",
        "activeTab",
        "contextMenus",
        "https://localhost/*",
        "http://here goes your localserverip/*"
        
    ]
}